{
  "name": "tree-sitter-weir",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for the Weir programming language",
  "tree-sitter": [
    {
      "scope": "source.weir",
      "file-types": ["weir"]
    }
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "test": "tree-sitter generate && tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "author": "Nathan Weir <nathan.weir@protonmail.com>",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.24.4",
    "prebuildify": "^6.0.0"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ]
}
