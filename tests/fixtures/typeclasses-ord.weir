;; Ord typeclass usage â€” Ordering/Ord/instances come from prelude

(defn min-of ((x : i32) (y : i32)) : i32
  (match (compare x y)
    (LT x) (EQ x) (GT y)))

(defn max-of ((x : i32) (y : i32)) : i32
  (match (compare x y)
    (GT x) (_ y)))

(defn main ()
  (println (min-of 10 3))
  (println (max-of 10 3)))
